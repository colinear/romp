{
  "name": "romp",
  "version": "1.0.0",
  "description": "An app for gamers to meet up and play.",
  "main": "server/index.js",
  "author": "colinear",
  "scripts": {
    "start": "cd client && react-scripts start && cd ../server && node index.js",
    "client": "cd client && react-scripts start",
    "server": "nodemon server/index.js",
    "build": "cd client && react-scripts build",
    "test": "cd client && react-scripts test --env=jsdom",
    "eject": "cd client && react-scripts eject",
    "build-css": "node-sass-chokidar ./client/src/styles/ -o ./client/src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar ./client/src/styles/ -o ./client/src/styles/ --watch --recursive"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt-nodejs": "0.0.3",
    "dateformat": "^3.0.2",
    "express": "^4.13.3",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "igdb-api-node": "^3.1.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.13.5",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "passport-twitch": "^1.0.3",
    "prop-types": "^15.6.0",
    "pure-react-carousel": "^1.9.11",
    "react-carousel": "^4.3.0",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-parallax": "^1.5.0",
    "react-parallax-mousemove": "^1.1.0",
    "react-redux": "^5.0.6",
    "react-responsive-carousel": "^3.1.29",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "react-star-rating-component": "^1.3.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-localstorage-simple": "^1.3.1",
    "redux-persist": "^5.4.0",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.0.1",
    "sass": "^1.0.0-beta.4",
    "semantic-ui": "^2.2.13",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.0",
    "socket.io": "^2.0.4",
    "twitch-embed": "^1.1.4"
  },
  "devDependencies": {
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "react-scripts": "1.0.17"
  },
  "engines": {
    "node": "6.10.0"
  },
  "peerDependencies": {
    "react": "^15.6.2"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "contributors": [
    "Cameron Malloy",
    "Cecilia Goss",
    "Clay Smart"
  ]
}
